SET ( PACKAGE_VERSION @OPENEXR_VERSION@ )

IF ( "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL @OPENEXR_VERSION_MAJOR@ )
  SET ( PACKAGE_VERSION_COMPATIBLE TRUE )

  IF ( "${PACKAGE_FIND_VERSION_MINOR}" EQUAL @OPENEXR_VERSION_MINOR@ AND "${PACKAGE_FIND_VERSION_MINOR}" EQUAL OPENEXR_VERSION_PATCH@ )
    SET ( PACKAGE_VERSION_EXACT TRUE )
  ENDIF ()
ELSE ()
  SET ( PACKAGE_VERSION_UNSUITABLE TRUE ) 
ENDIF ()


